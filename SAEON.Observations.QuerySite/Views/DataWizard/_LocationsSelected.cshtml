@using SAEON.Observations.QuerySite.Models
@model DataWizardModel
@(Html.EJ()
    .ListView("listViewLocationsSelected")
    .DataSource(Model.LocationNodesSelected.Where(i => i.Key.StartsWith("STA~")).ToList())
    .ShowHeader(true)
    .HeaderTitle("Selected")
    .FieldSettings(fs => fs.Text("Name"))
    .ClientSideEvents(events => events.LoadComplete("DataWizard.LocationsLoadComplete"))
)
