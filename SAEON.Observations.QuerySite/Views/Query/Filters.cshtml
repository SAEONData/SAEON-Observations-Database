@using SAEON.Observations.QuerySite.Models
@model QueryModel
<div class="row">
    <div class="col-md-2">
        @Html.LabelFor(i => i.StartDate)
    </div>
    <div class="col-md-10">
        @Html.EJ().DatePickerFor(i => i.StartDate).DateFormat("dd MMM yyyy")
    </div>
</div>
<div class="row">
    <div class="col-md-2">
        @Html.LabelFor(i => i.EndDate)
    </div>
    <div class="col-md-10">
        @Html.EJ().DatePickerFor(i => i.EndDate).DateFormat("dd MMM yyyy")
    </div>
</div>
